# Your wallet private key (without 0x prefix) - KEEP THIS SECRET!
PRIVATE_KEY=your_private_key_here

# Etherscan API key for contract verification
# Get from https://etherscan.io/myapikey - single key works for all supported chains including BSC
ETHERSCAN_API_KEY=your_etherscan_api_key

# Pinata API keys for IPFS (get from https://app.pinata.cloud/keys)
PINATA_API_KEY=your_pinata_api_key
PINATA_SECRET_KEY=your_pinata_secret_key
